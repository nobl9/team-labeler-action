{
  "name": "team-labeler-action",
  "version": "0.1.0",
  "private": true,
  "description": "Action to add a label based on the team",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "build:pack": "yarn build && yarn package",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulienKode/team-labeler-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "team",
    "label",
    "labeller",
    "node"
  ],
  "author": "Julien Karst",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "@types/jest": "29.1.2",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.8.4",
    "@typescript-eslint/parser": "5.40.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "8.25.0",
    "eslint-plugin-github": "4.4.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.1.2",
    "jest-circus": "29.1.2",
    "js-yaml": "3.14.1",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}
